{
  "_id": "_design/SpatialView",
  "language": "javascript",
  "st_indexes": {
    "geo": {
      "crs": {
        "type": "name",
        "properties": {
          "name": "urn:ogc:def:crs:OGC:1.3:CRS84"
        }
      },
      "index": "function(doc){\n if (doc.geometry) { st_index(doc.geometry); }\n}",
      "analyzer": "standard"
    }
  }
}
